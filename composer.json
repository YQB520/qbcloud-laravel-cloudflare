{
    "name": "qbcloud/laravel-cloudflare",
    "description": "A Laravel extension package for managing Cloudflare websites and domain names.",
    "keywords": [
        "laravel",
        "cloudflare",
        "website",
        "domain",
        "DNS"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "cloudflare/sdk": "^1.3",
        "illuminate/support": "^7|^8"
    },
    "autoload": {
        "psr-4": {
            "QbCloud\\Cloudflare\\": "src/"
        }
    },
    "authors": [
        {
            "name": "YQB520",
            "email": "604753396@qq.com"
        }
    ],
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "QbCloud\\Cloudflare\\Providers\\CloudflareServiceProvider"
            ],
            "aliases": {
                "Cloudflare": "QbCloud\\Cloudflare\\Facades\\Cloudflare"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
